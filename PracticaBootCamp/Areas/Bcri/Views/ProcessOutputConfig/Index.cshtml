@using DNF.Security.Bussines
@{var Res = PracticaBootCamp.Res.res;}

@model Bcri.Core.Bussines.ProcessConfig
    @{
        Layout = "";
    }

<div class="ibox">
    <div class="ibox-title">
        <div class="ibox-tools">
            <a class="collapse-link">
                <i class="fa fa-chevron-up"></i>
            </a>
        </div>
    </div>
    <div class="ibox-content">
        <table id="jqGridProcessOutputConfig"> </table>
        <div id="jqGridProcessOutputConfigPager"></div>
    </div>
</div>

<div id="ProcessOutputConfigJsData"
        data-dataUrl="@Url.Action("ProcessOutputData")"
        data-edit="@Current.User.HasAccess("ProcessOutputConfigEdit").ToString().ToLower()"
        data-editUrl="@Url.Action("ProcessOutputConfigEdit")"
        data-processconfigid="@Model.Id"
        data-optionsdataurl="@Url.Action("GetOptionsData")">
</div>

@section scripts {
    @Scripts.Render("~/Areas/Bcri/Views/ProcessOutputConfig/Index.js")
}
