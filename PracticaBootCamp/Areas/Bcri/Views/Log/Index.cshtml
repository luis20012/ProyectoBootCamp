@{var Res = PracticaBootCamp.Res.res;}
<div class="container-fluid">
	<div class="row">
		<h1 class="page-header">@Res.titLog</h1>

		<div class="row">
			<div class="form-group col-lg-4">
				@Html.DropDownList("types", (List<SelectListItem>)ViewBag.Types, new { @class = "form-control", placeholder = "Select a Type" })
			</div>
			<div class="form-group col-lg-4">
				<select id="actions" class="form-control" placeholder=@Res.placeHolderSelectAnAction></select>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="col-lg-12">
			<table id="jqGridProcess"> </table>
			<div id="jqGridProcessPager"></div>
		</div>
	</div>
</div>

@section scripts {
	@Scripts.Render("~/Log/Index.js")
}
