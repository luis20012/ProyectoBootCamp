@{
	var Res = Bcri.Web.Res.res;
	ViewBag.Title = Res.home;

}
<div class="col-lg-12">
    <div class="ibox">
        <div class="ibox-title">
            <h5>@ViewBag.Title</h5>
            <div class="ibox-tools">
                <a class="collapse-link">
                    <i class="fa fa-chevron-up"></i>
                </a>
            </div>
        </div>
        <div class="ibox-content">
            <div class="row container-fluid">
                <div class="jumbotron ">
                    <h1>BCRI CORE</h1>
                    <p class="lead">@Res.descriptionCore</p>
                </div>
            </div>
        </div>
    </div>
</div>